<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vaycent.vaycentproject">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name=".ApplicationContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <!--***************** Activity *****************-->
        <activity
            android:name=".DemoPackage.Volleysharp_demo"
            android:label="@string/app_name" />

        <activity
            android:name=".DemoPackage.OrmliteSharpDemo"
            android:label="@string/app_name" />
        <activity
            android:name=".DemoPackage.TextViewDemo"
            android:label="@string/app_name" />

        <activity
            android:name=".DemoPackage.NotificationDemo"
            android:label="@string/app_name" />

        <activity
            android:name=".DemoPackage.AnimationDemo"
            android:label="@string/app_name" />



        <!--***************** Service *****************-->
        <service android:name=".DemoPackage.NoKillService"/>


    </application>

</manifest>